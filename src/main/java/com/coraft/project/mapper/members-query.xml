<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="insertMember">
        INSERT INTO MEMBERS (
                                MEM_ID
                                , MEM_PWD
                                , MEM_NAME
                                , MEM_AGE
                                , MEM_GENDER
                                , PHONE
                                , EMAIL
                                , MEM_POINT
                            )
        VALUES (
                    ?
                    , ?
                    , ?
                    , ?
                    , ?
                    , ?
                    , ?
                    , ?
                )
    </entry>

    <entry key="selectIdPwd">
        SELECT MEM_ID
                , MEM_PWD
                , MEM_NAME
                , MEM_AGE
                , MEM_GENDER
                , PHONE
                , EMAIL
                , MEM_POINT
            FROM MEMBERS
            WHERE MEM_ID = ?
    </entry>

    <entry key="selectMemberInfo">
        SELECT <!--MEM_ID
                , MEM_PWD-->
                 MEM_NAME
                , MEM_AGE
                , MEM_GENDER
                , PHONE
                , EMAIL
                , MEM_POINT
            FROM MEMBERS
            <!--WHERE MEM_ID = ?-->
            <!--WHERE MEM_NAME = ?
                AND MEM_AGE = ?
                AND MEM_GENDER = ?
                AND PHONE = ?
                AND EMAIL = ?
                AND MEM_POINT = ?-->
    </entry>
</properties>